{
    "name": "esimonetti/toothpaste",
    "description": "CLI utility to analyse, optimise and provide additional functionality to your Sugar system",
    "type": "package",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Enrico Simonetti",
            "email": "enrico.simonetti@gmail.com",
            "homepage": "http://enricosimonetti.com"
        }
    ],
    "bin": [
        "bin/toothpaste"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://github.com/hvu-sugarcrm/toothpaste.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/console": "4.3.*",
        "guzzlehttp/guzzle": "6.4.*",
        "esimonetti/toothpaste":"dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Toothpaste\\": "src/"
        }
    }
}
